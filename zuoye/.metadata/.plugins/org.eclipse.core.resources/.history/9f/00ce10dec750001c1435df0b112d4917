package leibulei6;

public class MathDemo {

	public static void main(String[] args) {
          //public static int abs (int a):返回参数的绝对值
		System.out.println(Math.abs(99));
		System.out.println(Math.abs(-99));
		System.out.println("--------");
		//public static double ceil(double a): 返回值大于或者等于参数的最小值double值，等于一个整数
		System.out.println(Math.ceil(12.34));
		System.out.println(Math.ceil(12.56));
		System.out.println("--------");
		//public static double floor (double a):返回小于或者等于参数的最大值double值，等于一个整数
		System.out.println(Math.floor(12.34));
		System.out.println(Math.floor(12.65));
		System.out.println("--------");
		//public static int max (int a,int b),返回两个int值中的最大值
		System.out.println(Math.max(66,44));
		System.out.println(Math.max(99,44));
		System.out.println("-----------");
		//public static int min (int a,int b),返回两个int值中的最大值
		System.out.println(Math.min(35, 0));
		System.out.println(Math.min(99, 0));
		System.out.println("------------");
		//public static int round (float a):按照四舍五入返回最接近参数的int
	    System.out.println(Math.round(12.34F));
	    System.out.println(Math.round(12.56F));
	    System.out.println("----------------");
	    //public static double pow(double a,doule b):返回a的b次幂值
	    System.out.println(Math.pow(2.0,3.0));
	    System.out.println("---------------");
	    //public static double random();返回值为double的值，[0.0,1.0)
	    System.out.println(Math.random());
	    System.out.println((int)(Math.random()*100)+1);
	    
	    
	
	}

}
