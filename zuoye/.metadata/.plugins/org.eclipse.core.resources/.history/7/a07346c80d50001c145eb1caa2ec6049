package inthetouge_1;

public class Student extends Person{
        public  String school;
	
	public Student() {
    	
    }
    public Student(String name,int age) {
    	super(name, age);
    	this.school="哈佛大学";
    
    }
    public void talk() {
    	System.out.println("我是："+getName()+","+"今年："+getAge()+"岁，我在"+school+"上学");
    }
	
}
