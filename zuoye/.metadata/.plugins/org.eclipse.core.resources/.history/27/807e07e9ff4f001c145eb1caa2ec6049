package inthetouge_1;

public class Animal {
      private String name;
      private int age;
     public Animal() {
    	 
     }
 public Animal(String name, int age) {
    	 this.name=name;
    	 this.age=age;
    	 
     }
	public void eat() {
    	  System.out.println("动物吃东西");
      }
      public void voice() {
    	  System.out.println("动物叫");
      }
}
