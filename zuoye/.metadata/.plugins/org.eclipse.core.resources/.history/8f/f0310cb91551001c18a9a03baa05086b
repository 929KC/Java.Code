package System.out;

public class Dog extends Pet {
	
	private String color;
	public Dog() {

	}

	public Dog(String name,String sex,String color) {
		super(name,sex);
		
		this.color=color;
		
	}

	public void talk() {
		System.out.println("名称："+getName()+ "，性别："+getSex()+"，颜色："+color+"，汪汪叫" );
	}

	public void eat() {
		System.out.println(getName()+"吃骨头！");
	}
	
	
}
