package inthetouge_2;

public class Demo {

	public static void main(String[] args) {
		int x=90;
		if(x>0)
		    if(x>100)
		        System.out.print("大于100");
		    else if (x>50)
		        System.out.print("在 50 到 100 之间");
		    else if (x<50) {
		        System.out.print("在 0 到 50 之间");
		    } else 
		        System.out.print("等于 50");

	}

}
