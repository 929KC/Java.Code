package java_1;

import java.util.Scanner;

public class HelloWorld {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in); 
		final float PI=3.14f;
		float r ,sum=0.0;
		System.out.println("请输入半径r的值：");
            r=sc.nextFloat();
		System.out.print("圆的面积是:"+(sum=PI*r*r));
     }

	}


