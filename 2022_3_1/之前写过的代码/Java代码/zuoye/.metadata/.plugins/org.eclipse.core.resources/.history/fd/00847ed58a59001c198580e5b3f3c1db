package inthe头哥12_10;

import java.util.Scanner;

public class Zodiac {

	public static void main(String[] args) {
		   Scanner input = new Scanner(System.in);
		  int k=1;
		   while(k<=12) {
			   System.out.println("请输入"+"第"+k+"年份");
			   int year = input.nextInt();
		        System.out.println(year%12);
		        k++;
		   }

	}

}
